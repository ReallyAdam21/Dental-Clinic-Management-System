{
    "extends": "./tsconfig.json",
    
       "compilerOptions": {
    "target": "ES2020",
    "module": "commonjs",
    "lib": ["ES2020"],
    "outDir": "out/main",
    "rootDir": "src/main",
    "strict": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "node"
  },
  "include": ["src/main/**/*"],
  "exclude": ["node_modules", "out", "dist"]
}